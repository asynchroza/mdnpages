.SH EXAMPLES
.SS Custom string search
.IP
.nf
\f[C]
class caseInsensitiveSearch {
  constructor(value) {
    this.value = value.toLowerCase();
  }
  [Symbol.search](string) {
    return string.toLowerCase().indexOf(this.value);
  }
}

console.log(\[dq]foobar\[dq].search(new caseInsensitiveSearch(\[dq]BaR\[dq]))); // 3
\f[R]
