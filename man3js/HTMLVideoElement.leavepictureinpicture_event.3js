const button = document.querySelector(\[dq]#button\[dq]);

function onExitPip() {
  console.log(\[dq]Picture\-in\-Picture mode deactivated!\[dq]);
}

video.onleavepictureinpicture = onExitPip;

button.onclick = () => {
  if (document.pictureInPictureElement) {
    document.exitPictureInPicture();
  }
};
.EE
