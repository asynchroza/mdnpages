.EE
.IP
.EX
const unitType = [\[dq]unknown\[dq], \[dq]userSpaceOnUse\[dq], \[dq]objectBoundingBox\[dq]];

const mask = document.getElementById(\[dq]mask2\[dq]);
const log = document.getElementById(\[dq]log\[dq]);

function displayLog() {
  const baseValue = unitType[mask.maskUnits.baseVal];
  const animValue = unitType[mask.maskUnits.animVal];
  log.textContent = \[ga]The top\-right \[aq]maskUnits.baseVal\[aq] coord system : ${baseValue}\[rs]n\[ga];
  log.textContent += \[ga]The top\-right \[aq]maskUnits.animVal\[aq] coord system : ${animValue}\[ga];
  requestAnimationFrame(displayLog);
}
displayLog();
.EE
