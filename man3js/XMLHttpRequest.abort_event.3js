.EX
const xhrButtonSuccess = document.querySelector(\[dq].xhr.success\[dq]);
const xhrButtonError = document.querySelector(\[dq].xhr.error\[dq]);
const xhrButtonAbort = document.querySelector(\[dq].xhr.abort\[dq]);
const log = document.querySelector(\[dq].event\-log\[dq]);

function handleEvent(e) {
  log.textContent = \[ga]${log.textContent}${e.type}: ${e.loaded} bytes transferred\[rs]n\[ga];
}

function addListeners(xhr) {
  xhr.addEventListener(\[dq]loadstart\[dq], handleEvent);
  xhr.addEventListener(\[dq]load\[dq], handleEvent);
  xhr.addEventListener(\[dq]loadend\[dq], handleEvent);
  xhr.addEventListener(\[dq]progress\[dq], handleEvent);
  xhr.addEventListener(\[dq]error\[dq], handleEvent);
  xhr.addEventListener(\[dq]abort\[dq], handleEvent);
}

function runXHR(url) {
  log.textContent = \[dq]\[dq];

  const xhr = new XMLHttpRequest();
  addListeners(xhr);
  xhr.open(\[dq]GET\[dq], url);
  xhr.send();
  return xhr;
}

xhrButtonSuccess.addEventListener(\[dq]click\[dq], () => {
  runXHR(\[dq]my\-picture.jpg\[dq]);
});

xhrButtonError.addEventListener(\[dq]click\[dq], () => {
  runXHR(\[dq]https://example.com/some\-path\[dq]);
});

xhrButtonAbort.addEventListener(\[dq]click\[dq], () => {
  runXHR(\[dq]my\-picture.jpg\[dq]).abort();
});
